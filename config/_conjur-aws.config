## Run Master cluster in AWS docker daemon
export NO_DNS=false
export CONJUR_MASTER_HOST_NAME=<public-dns-name>
export CONJUR_MASTER_HOST_IP=<public-ip-address>
export CONJUR_MASTER_PORT=443
export CONJUR_MASTER_PGSYNC_PORT=5432
export CONJUR_MASTER_PGAUDIT_PORT=5433
export CONJUR_MASTER_HOST_ADMIN=ec2-user
export CONJUR_MASTER_SSH_KEY=<ssh-key-for-ec2-instance>
