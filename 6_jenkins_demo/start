if [[ "$DOCKER_HOST" != "" ]]; then
  # use localhost docker daemon for docker-compose support
  unset DOCKER_TLS_VERIFY
  unset DOCKER_HOST
  unset DOCKER_CERT_PATH
fi
docker-compose up -d
# copy Conjur network config info to demo bootstrap file
cp ../config/conjur.config demo/bootstrap.env
cp ../config/_conjur-*.config demo/
echo "export CONJUR_HOST=\$CONJUR_MASTER_HOST_NAME:\$CONJUR_MASTER_PORT" >> demo/bootstrap.env
sleep 2
echo "cd to /demo and run scripts in order"
docker exec -it jenkins-master bash
